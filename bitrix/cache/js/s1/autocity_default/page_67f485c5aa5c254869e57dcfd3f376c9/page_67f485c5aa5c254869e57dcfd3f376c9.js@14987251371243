$(document).ready(function(){$(document).on("click",".vacancies .filter .btn",function(){var n=$(this).data("filter");$(".vacancies .filter .btn").addClass("btn-default").removeClass("btn-primary"),$(this).addClass("btn-primary").removeClass("btn-default"),$(".vacancies").find(".panel-group .item .panel-heading").find("a").addClass("collapsed"),$(".vacancies").find(".panel-group .item .panel-body").parent().removeClass("in"),""==n?$(".vacancies").find(".panel-group").find(".item").show():($(".vacancies").find(".panel-group").find(".item").hide(),$(".vacancies").find(".panel-group").find(".item.filter"+n).show())}),$(".vacanciesForm").find('input[name="RS_EXT_FIELD_0"]').length>0&&$(document).on("click",".btn-respond",function(){var n=$(this).data("vacancy");$(".vacanciesForm").show().find('input[name="RS_EXT_FIELD_0"]').val(n)})});
